  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "devDependencies": {
    "@lintest/core": "^1.4.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.47",
