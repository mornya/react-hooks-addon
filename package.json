  "repository": "https://github.com/mornya/react-hooks-addon.git",
  "main": "./dist/index.js",
  "types": "./dist",
  "scripts": {
    "setup": "husky install",
    "prepublishOnly": "vessel prepublish",
    "login": "vessel login",
    "clean": "vessel clean",
    "build": "vessel build",
    "watch": "vessel build --watch",
    "check": "vessel check",
    "lint": "lintest lint",
    "lint:fix": "lintest lint --fix",
